{
    "openapi": "3.1.0",
    "info": {
        "title": "FastAPI",
        "version": "0.1.0"
    },
    "paths": {
        "/route": {
            "post": {
                "summary": "Plan Route",
                "operationId": "plan_route_route_post",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "$ref": "#/components/schemas/TourRequest"
                            }
                        }
                    },
                    "required": true
                },
                "responses": {
                    "200": {
                        "description": "Successful Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/TourResponse"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "Validation Error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/HTTPValidationError"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "HTTPValidationError": {
                "properties": {
                    "detail": {
                        "items": {
                            "$ref": "#/components/schemas/ValidationError"
                        },
                        "type": "array",
                        "title": "Detail"
                    }
                },
                "type": "object",
                "title": "HTTPValidationError"
            },
            "Route": {
                "properties": {
                    "order": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array",
                        "title": "Order"
                    },
                    "distance_km": {
                        "type": "number",
                        "title": "Distance Km"
                    },
                    "duration_min": {
                        "type": "number",
                        "title": "Duration Min"
                    },
                    "path": {
                        "items": {
                            "items": {
                                "type": "number"
                            },
                            "type": "array"
                        },
                        "type": "array",
                        "title": "Path"
                    }
                },
                "type": "object",
                "required": [
                    "order",
                    "distance_km",
                    "duration_min",
                    "path"
                ],
                "title": "Route"
            },
            "Stop": {
                "properties": {
                    "name": {
                        "type": "string",
                        "title": "Name"
                    },
                    "lat": {
                        "anyOf": [
                            {
                                "type": "number"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Lat"
                    },
                    "lon": {
                        "anyOf": [
                            {
                                "type": "number"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Lon"
                    }
                },
                "type": "object",
                "required": [
                    "name"
                ],
                "title": "Stop"
            },
            "TourRequest": {
                "properties": {
                    "stops": {
                        "items": {
                            "$ref": "#/components/schemas/Stop"
                        },
                        "type": "array",
                        "title": "Stops"
                    },
                    "optimize": {
                        "anyOf": [
                            {
                                "type": "boolean"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Optimize",
                        "default": true
                    }
                },
                "type": "object",
                "required": [
                    "stops"
                ],
                "title": "TourRequest"
            },
            "TourResponse": {
                "properties": {
                    "optimized_route": {
                        "$ref": "#/components/schemas/Route"
                    },
                    "alternatives": {
                        "anyOf": [
                            {
                                "items": {
                                    "$ref": "#/components/schemas/Route"
                                },
                                "type": "array"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "title": "Alternatives"
                    }
                },
                "type": "object",
                "required": [
                    "optimized_route"
                ],
                "title": "TourResponse"
            },
            "ValidationError": {
                "properties": {
                    "loc": {
                        "items": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "integer"
                                }
                            ]
                        },
                        "type": "array",
                        "title": "Location"
                    },
                    "msg": {
                        "type": "string",
                        "title": "Message"
                    },
                    "type": {
                        "type": "string",
                        "title": "Error Type"
                    }
                },
                "type": "object",
                "required": [
                    "loc",
                    "msg",
                    "type"
                ],
                "title": "ValidationError"
            }
        }
    }
}